      <Text layoutX="155.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="155.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <PasswordField fx:id="passwordField" layoutX="156.0" layoutY="188.0" prefHeight="25.0" prefWidth="290.0" />
      <Button fx:id="registerButton" layoutX="243.0" layoutY="332.0" mnemonicParsing="false" text="Registrer ny bruker" />
      <Button fx:id="loginButton" layoutX="270.0" layoutY="262.0" mnemonicParsing="false" text="Logg inn" onAction="#loginButtonClicked" />
